{
  "css.validate": false,
  "scss.validate": false,
  "less.validate": false,
  "css.lint.unknownAtRules": "ignore",
  "tailwindCSS.includeLanguages": {
    "plaintext": "css",
    "postcss": "css"
  },
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.experimental.configFile": "tailwind.config.js",
  "sqltools.connections": [
