    {
      "name": "FinScore Docker PostgreSQL",
      "driver": "PostgreSQL",
      "previewLimit": 50,
      "server": "localhost",
      "port": 5432,
      "database": "finscore_db",
      "username": "postgres",
      "password": "postgres",
      "connectionTimeout": 10
    }
  ]
